{
  "name": "@naloxonemap/data-faker",
  "version": "1.1.0",
  "description": "Generate fake data for the Naloxone Map project.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "test": "jest --config jestconfig.json --coverage",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NaloxoneMap/data-faker.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "author": "Charles Desbiens",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/NaloxoneMap/data-faker/issues"
  },
  "homepage": "https://github.com/NaloxoneMap/data-faker#readme",
  "devDependencies": {
    "@types/jest": "^26.0.0",
    "@types/json-schema-faker": "^0.5.0",
    "jest": "^26.0.1",
    "prettier": "^2.0.5",
    "ts-jest": "^26.1.0",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.9.5"
  },
  "dependencies": {
    "faker": "^4.1.0",
    "json-schema-faker": "^0.5.0-rcv.24"
  }
}
